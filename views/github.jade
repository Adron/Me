                      li.tab
                        a.home(href='../')
                          span.icon-wrap
                            i.icon-home
                          | Home @Adron
                    .panel-container
                      #coderep.page
                        .content-wrap
                          .content.seven
                            .content-pad
                              h3.org-head
                                span Github Projects
